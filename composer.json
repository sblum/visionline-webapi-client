{
    "name": "sebastianblum/visionline-webapi-client",
    "type": "project",
    "description": "Extraction of the visionline webapi client for the crm from visionline.at",
    "autoload": {
        "psr-0": { 
          "Visionline\\Installer": "src/",
          "Visionline\\Crm\\WebApi": "src/" 
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Visionline\\Installer\\InstallHandler::extractPhar"
        ],
        "post-update-cmd": [
            "Visionline\\Installer\\InstallHandler::extractPhar"
        ]
    },
    "require": {
      "php": ">=5.3.3"
    }
}
